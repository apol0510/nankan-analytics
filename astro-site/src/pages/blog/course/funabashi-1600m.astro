                        ctx.fillText(value.toFixed(1) + '%', x + barWidth * 0.4, y - 5);
                    }

                    // 7ç•ªæ ã®è¤‡å‹ç‡ç‰¹è¨˜
                    if (index === 6) {
                        ctx.fillStyle = '#8e44ad';
                        ctx.font = 'bold 9px sans-serif';
                        ctx.fillText('è¤‡40%', x + barWidth * 0.4, y - 18);
                    }

                    // ã‚µãƒ³ãƒ—ãƒ«æ•°ãŒå°‘ãªã„æ ã«æ³¨è¨˜
                    if (samples[index] < 30) {
                        ctx.fillStyle = '#e74c3c';
                        ctx.font = '8px sans-serif';
                        ctx.textAlign = 'center';
                        ctx.fillText('(' + samples[index] + ')', x + barWidth * 0.4, y + 15);
                    }

                    // ãƒ©ãƒ™ãƒ«è¡¨ç¤º
                    ctx.fillStyle = '#34495e';
                    ctx.font = '9px sans-serif';
                    ctx.textAlign = 'center';
                    ctx.save();
                    ctx.translate(x + barWidth * 0.4, height - 15);
                    ctx.rotate(-Math.PI / 6);
                    ctx.fillText(labels[index], 0, 0);
                    ctx.restore();
                });

                // è»¸ç·š
                ctx.strokeStyle = '#2c3e50';
                ctx.lineWidth = 2;
                ctx.beginPath();
                ctx.moveTo(padding, height - padding);
                ctx.lineTo(width - padding, height - padding);
                ctx.stroke();

                // ã‚¿ã‚¤ãƒˆãƒ«
                ctx.fillStyle = '#2c3e50';
                ctx.font = 'bold 14px sans-serif';
                ctx.textAlign = 'left';
                ctx.fillText('å‹ç‡(%)', 10, 20);

                // æ³¨è¨˜
                ctx.fillStyle = '#8e44ad';
                ctx.font = '10px sans-serif';
                ctx.fillText('â€»14ç•ªæ ã¯ã‚µãƒ³ãƒ—ãƒ«å°‘', width - 140, 20);
            };

            // äººæ°—åˆ¥æˆç¸¾ã‚°ãƒ©ãƒ•
            const drawPopularityChart = () => {
                const canvas = document.getElementById('popularityChart');
                if (!canvas) {
                    console.log('äººæ°—ãƒãƒ£ãƒ¼ãƒˆã®canvasè¦ç´ ãŒè¦‹ã¤ã‹ã‚Šã¾ã›ã‚“');
                    return;
                }

                console.log('äººæ°—ãƒãƒ£ãƒ¼ãƒˆã‚’æç”»é–‹å§‹');
                hideLoading('popularityChartLoading');

                const ctx = canvas.getContext('2d');
                const popularityData = [43.4, 22.6, 10.4, 7.5, 3.8, 2.0];
                const fukushoData = [71.7, 54.7, 39.6, 37.7, 25.5, 11.5];
                const popLabels = ['1ç•ªäººæ°—', '2ç•ªäººæ°—', '3ç•ªäººæ°—', '4ç•ªäººæ°—', '5ç•ªäººæ°—', '6ç•ªäººæ°—ä»¥ä¸‹'];

                // ãƒ¬ã‚¹ãƒãƒ³ã‚·ãƒ–å¯¾å¿œ
                const containerWidth = canvas.parentElement.offsetWidth;
                const devicePixelRatio = window.devicePixelRatio || 1;

                canvas.width = containerWidth * devicePixelRatio;
                canvas.height = 400 * devicePixelRatio;
                canvas.style.width = containerWidth + 'px';
                canvas.style.height = '400px';

                ctx.scale(devicePixelRatio, devicePixelRatio);

                const width = containerWidth;
                const height = 400;
                const padding = 40;
                const barWidth = (width - padding * 2) / popularityData.length;

                // èƒŒæ™¯
                ctx.fillStyle = '#ffffff';
                ctx.fillRect(0, 0, width, height);

                // ã‚°ãƒ©ãƒ•æç”»
                popularityData.forEach((value, index) => {
                    const barHeight = (value / 50) * (height - padding * 2);
                    const x = padding + index * barWidth + barWidth * 0.15;
                    const y = height - padding - barHeight;

                    // ãƒãƒ¼ã®è‰²åˆ†ã‘
                    if (index === 0) {
                        // 1ç•ªäººæ°—
                        const gradient = ctx.createLinearGradient(0, y, 0, height - padding);
                        gradient.addColorStop(0, '#8e44ad');
                        gradient.addColorStop(1, '#9b59b6');
                        ctx.fillStyle = gradient;
                    } else if (index === 1) {
                        // 2ç•ªäººæ°—
                        ctx.fillStyle = '#ba68c8';
                    } else if (index === 2) {
                        // 3ç•ªäººæ°—
                        ctx.fillStyle = '#ce93d8';
                    } else if (index === 3) {
                        // 4ç•ªäººæ°—
                        ctx.fillStyle = '#e1bee7';
                    } else if (index === 5) {
                        // 6ç•ªäººæ°—ä»¥ä¸‹
                        ctx.fillStyle = '#e0e0e0';
                    } else {
                        // ãã®ä»–
                        ctx.fillStyle = '#f3e5f5';
                    }

                    // ãƒãƒ¼æç”»
                    ctx.fillRect(x, y, barWidth * 0.7, barHeight || 2);

                    // å€¤è¡¨ç¤º
                    ctx.fillStyle = '#2c3e50';
                    ctx.font = 'bold 11px sans-serif';
                    ctx.textAlign = 'center';
                    ctx.fillText(value.toFixed(1) + '%', x + barWidth * 0.35, y - 5);

                    // è¤‡å‹ç‡è¡¨ç¤ºï¼ˆ1-3ç•ªäººæ°—ï¼‰
                    if (index === 0) {
                        ctx.fillStyle = '#8e44ad';
                        ctx.font = 'bold 10px sans-serif';
                        ctx.fillText('è¤‡å‹71.7%', x + barWidth * 0.35, y - 20);
                    } else if (index === 1) {
                        ctx.fillStyle = '#9b59b6';
                        ctx.font = 'bold 10px sans-serif';
                        ctx.fillText('è¤‡å‹54.7%', x + barWidth * 0.35, y - 20);
                    } else if (index === 2 || index === 3) {
                        ctx.fillStyle = '#ba68c8';
                        ctx.font = '9px sans-serif';
                        ctx.fillText('è¤‡' + fukushoData[index].toFixed(1) + '%', x + barWidth * 0.35, y - 20);
                    }

                    // ãƒ©ãƒ™ãƒ«è¡¨ç¤º
                    ctx.fillStyle = '#34495e';
                    ctx.font = '9px sans-serif';
                    ctx.fillText(popLabels[index], x + barWidth * 0.35, height - 20);
                });

                // è»¸ç·š
                ctx.strokeStyle = '#2c3e50';
                ctx.lineWidth = 2;
                ctx.beginPath();
                ctx.moveTo(padding, height - padding);
                ctx.lineTo(width - padding, height - padding);
                ctx.stroke();

                // ã‚¿ã‚¤ãƒˆãƒ«
                ctx.fillStyle = '#2c3e50';
                ctx.font = 'bold 14px sans-serif';
                ctx.textAlign = 'left';
                ctx.fillText('å‹ç‡(%)', 10, 20);
            };

            // ã‚°ãƒ©ãƒ•ã‚’æç”»
            drawHorseChart();
            drawPopularityChart();

            // ãƒªã‚µã‚¤ã‚ºå¯¾å¿œ
            let resizeTimer;
            window.addEventListener('resize', () => {
                clearTimeout(resizeTimer);
                resizeTimer = setTimeout(() => {
                    drawHorseChart();
                    drawPopularityChart();
                }, 250);
            });
        }

        // è¤‡æ•°ã®å®Ÿè¡Œã‚¿ã‚¤ãƒŸãƒ³ã‚°ã«å¯¾å¿œ
        if (document.readyState === 'loading') {
            document.addEventListener('DOMContentLoaded', initCharts);
        } else {
            initCharts();
        }

        // Astroã®ãƒšãƒ¼ã‚¸é·ç§»å¯¾å¿œ
        document.addEventListener('astro:page-load', initCharts);
    </script>
