    // äººæ°—åˆ¥ã‚°ãƒ©ãƒ•æç”»
    function drawPopularityChart() {
      const canvas = document.getElementById('popularityChart');
      if (!canvas) return;

      const ctx = canvas.getContext('2d');
      const width = canvas.width;
      const height = canvas.height;

      // Clear canvas
      ctx.clearRect(0, 0, width, height);

      // è¨­å®š
      const padding = { top: 40, right: 40, bottom: 60, left: 60 };
      const chartWidth = width - padding.left - padding.right;
      const chartHeight = height - padding.top - padding.bottom;
      const barWidth = chartWidth / (popularityData.length * 3 + 2);
      const maxRate = 100;

      // èƒŒæ™¯
      ctx.fillStyle = '#f8f9fa';
      ctx.fillRect(0, 0, width, height);

      // ã‚°ãƒªãƒƒãƒ‰ç·š
      ctx.strokeStyle = '#e0e0e0';
      ctx.lineWidth = 1;

      for (let i = 0; i <= 5; i++) {
        const y = padding.top + (chartHeight * i / 5);
        ctx.beginPath();
        ctx.moveTo(padding.left, y);
        ctx.lineTo(width - padding.right, y);
        ctx.stroke();

        // Yè»¸ãƒ©ãƒ™ãƒ«
        ctx.fillStyle = '#666';
        ctx.font = '12px sans-serif';
        ctx.textAlign = 'right';
        ctx.fillText(`${maxRate - (i * 20)}%`, padding.left - 10, y + 4);
      }

      // ãƒ‡ãƒ¼ã‚¿æç”»
      popularityData.forEach((data, index) => {
        const x = padding.left + (index * 3 + 1) * barWidth;

        // å‹ç‡ï¼ˆèµ¤ï¼‰
        const winHeight = (data.winRate / maxRate) * chartHeight;
        ctx.fillStyle = '#dc2626';
        ctx.fillRect(x, padding.top + chartHeight - winHeight, barWidth * 0.8, winHeight);

        // é€£å¯¾ç‡ï¼ˆã‚ªãƒ¬ãƒ³ã‚¸ï¼‰
        const placeHeight = (data.placeRate / maxRate) * chartHeight;
        ctx.fillStyle = '#f97316';
        ctx.fillRect(x + barWidth, padding.top + chartHeight - placeHeight, barWidth * 0.8, placeHeight);

        // 3ç€å†…ç‡ï¼ˆé’ï¼‰
        const showHeight = (data.showRate / maxRate) * chartHeight;
        ctx.fillStyle = '#3b82f6';
        ctx.fillRect(x + barWidth * 2, padding.top + chartHeight - showHeight, barWidth * 0.8, showHeight);

        // Xè»¸ãƒ©ãƒ™ãƒ«
        ctx.fillStyle = '#333';
        ctx.font = '11px sans-serif';
        ctx.textAlign = 'center';
        ctx.save();
        ctx.translate(x + barWidth * 1.5, height - padding.bottom + 15);
        ctx.rotate(-Math.PI / 6);
        ctx.fillText(data.rank, 0, 0);
        ctx.restore();
      });

      // ã‚¿ã‚¤ãƒˆãƒ«
      ctx.fillStyle = '#1f2937';
      ctx.font = 'bold 16px sans-serif';
      ctx.textAlign = 'center';
      ctx.fillText('äººæ°—åˆ¥æˆç¸¾ï¼ˆæµ¦å’Œ1500mï¼‰', width / 2, 25);

      // å‡¡ä¾‹
      const legendY = height - 20;
      const legendItems = [
        { color: '#dc2626', label: 'å‹ç‡' },
        { color: '#f97316', label: 'é€£å¯¾ç‡' },
        { color: '#3b82f6', label: '3ç€å†…ç‡' }
      ];

      let legendX = width / 2 - 100;
      legendItems.forEach(item => {
        ctx.fillStyle = item.color;
        ctx.fillRect(legendX, legendY - 10, 15, 10);
        ctx.fillStyle = '#666';
        ctx.font = '12px sans-serif';
        ctx.textAlign = 'left';
        ctx.fillText(item.label, legendX + 20, legendY);
        legendX += 70;
      });
    }

    // ãƒãƒƒã‚¯ãƒˆã‚¥ãƒˆãƒƒãƒ—ãƒœã‚¿ãƒ³
    document.addEventListener('DOMContentLoaded', function() {
      const backToTopBtn = document.getElementById('back-to-top');

      // ã‚¹ã‚¯ãƒ­ãƒ¼ãƒ«æ™‚ã®è¡¨ç¤º/éè¡¨ç¤º
      window.addEventListener('scroll', function() {
        if (window.pageYOffset > 300) {
          backToTopBtn.classList.add('visible');
        } else {
          backToTopBtn.classList.remove('visible');
        }
      });

      // ã‚¯ãƒªãƒƒã‚¯ã§ãƒˆãƒƒãƒ—ã¸
      backToTopBtn.addEventListener('click', function() {
        window.scrollTo({
          top: 0,
          behavior: 'smooth'
        });
      });

      // ã‚°ãƒ©ãƒ•æç”»
      drawHorseNumberChart();
      drawPopularityChart();

      // ãƒªã‚µã‚¤ã‚ºå¯¾å¿œ
      let resizeTimer;
      window.addEventListener('resize', function() {
        clearTimeout(resizeTimer);
        resizeTimer = setTimeout(function() {
          drawHorseNumberChart();
          drawPopularityChart();
        }, 250);
      });

      // ç›®æ¬¡ã®ã‚¹ãƒ ãƒ¼ã‚¹ã‚¹ã‚¯ãƒ­ãƒ¼ãƒ«
      const tocLinks = document.querySelectorAll('.table-of-contents a');
      tocLinks.forEach(link => {
        link.addEventListener('click', function(e) {
          e.preventDefault();
          const targetId = this.getAttribute('href').substring(1);
          const targetElement = document.getElementById(targetId);
          if (targetElement) {
            const offset = 80;
            const targetPosition = targetElement.offsetTop - offset;
            window.scrollTo({
              top: targetPosition,
              behavior: 'smooth'
            });
          }
        });
      });

      // ã‚¹ã‚¯ãƒ­ãƒ¼ãƒ«ä½ç½®ã«å¿œã˜ãŸç›®æ¬¡ã®ã‚¢ã‚¯ãƒ†ã‚£ãƒ–çŠ¶æ…‹æ›´æ–°
      const sections = document.querySelectorAll('h2[id]');
      window.addEventListener('scroll', function() {
        let current = '';
        sections.forEach(section => {
          const sectionTop = section.offsetTop;
          const sectionHeight = section.offsetHeight;
          if (window.pageYOffset >= sectionTop - 100) {
            current = section.getAttribute('id');
          }
        });

        tocLinks.forEach(link => {
          link.classList.remove('active');
          if (link.getAttribute('href') === '#' + current) {
            link.classList.add('active');
          }
        });
      });
    });
