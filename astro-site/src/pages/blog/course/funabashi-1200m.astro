            backToTopButton.addEventListener('click', function() {
                window.scrollTo({
                    top: 0,
                    behavior: 'smooth'
                });
            });

            // ãƒãƒ£ãƒ¼ãƒˆæç”»é–¢æ•°
            function drawHorseChart() {
                const canvas = document.getElementById('horseNumberChart');
                if (!canvas) return;

                const ctx = canvas.getContext('2d');
                const winRates = [7.0, 10.5, 8.8, 9.6, 6.8, 9.9, 9.4, 10.7, 12.2, 11.6, 11.4, 8.2];
                const labels = ['1ç•ª', '2ç•ª', '3ç•ª', '4ç•ª', '5ç•ª', '6ç•ª', '7ç•ª', '8ç•ª', '9ç•ª', '10ç•ª', '11ç•ª', '12ç•ª'];

                canvas.width = 800;
                canvas.height = 400;

                const padding = 60;
                const barWidth = (canvas.width - padding * 2) / winRates.length;
                const maxValue = Math.max(...winRates);

                // èƒŒæ™¯ã‚’ã‚¯ãƒªã‚¢
                ctx.fillStyle = '#ffffff';
                ctx.fillRect(0, 0, canvas.width, canvas.height);

                // ãƒãƒ¼ã‚°ãƒ©ãƒ•ã‚’æç”»
                winRates.forEach((value, index) => {
                    const barHeight = (value / maxValue) * (canvas.height - padding * 2) * 0.8;
                    const x = padding + index * barWidth + barWidth * 0.2;
                    const y = canvas.height - padding - barHeight;

                    // ãƒãƒ¼ã®è‰²ã‚’æ±ºå®š
                    if (index === 8) { // 9ç•ªæ 
                        ctx.fillStyle = '#e74c3c';
                    } else if ([7, 9, 10].includes(index)) { // 8, 10, 11ç•ªæ 
                        ctx.fillStyle = '#3498db';
                    } else if (index === 4) { // 5ç•ªæ 
                        ctx.fillStyle = '#95a5a6';
                    } else {
                        ctx.fillStyle = '#bdc3c7';
                    }

                    ctx.fillRect(x, y, barWidth * 0.6, barHeight);

                    // æ•°å€¤è¡¨ç¤º
                    ctx.fillStyle = '#2c3e50';
                    ctx.font = 'bold 12px sans-serif';
                    ctx.textAlign = 'center';
                    ctx.fillText(value.toFixed(1) + '%', x + barWidth * 0.3, y - 10);

                    // ãƒ©ãƒ™ãƒ«è¡¨ç¤º
                    ctx.fillStyle = '#34495e';
                    ctx.font = '11px sans-serif';
                    ctx.fillText(labels[index], x + barWidth * 0.3, canvas.height - 30);
                });

                // ã‚¿ã‚¤ãƒˆãƒ«
                ctx.fillStyle = '#2c3e50';
                ctx.font = 'bold 16px sans-serif';
                ctx.textAlign = 'left';
                ctx.fillText('é¦¬ç•ªåˆ¥å‹ç‡(%)', 20, 30);
            }

            function drawPopularityChart() {
                const canvas = document.getElementById('popularityChart');
                if (!canvas) return;

                const ctx = canvas.getContext('2d');
                const winRates = [47.6, 17.5, 14.3, 8.0, 4.6, 1.5];
                const labels = ['1ç•ªäººæ°—', '2ç•ªäººæ°—', '3ç•ªäººæ°—', '4ç•ªäººæ°—', '5ç•ªäººæ°—', '6ç•ªäººæ°—ä»¥ä¸‹'];

                canvas.width = 800;
                canvas.height = 400;

                const padding = 60;
                const barWidth = (canvas.width - padding * 2) / winRates.length;
                const maxValue = Math.max(...winRates);

                // èƒŒæ™¯ã‚’ã‚¯ãƒªã‚¢
                ctx.fillStyle = '#ffffff';
                ctx.fillRect(0, 0, canvas.width, canvas.height);

                // ãƒãƒ¼ã‚°ãƒ©ãƒ•ã‚’æç”»
                winRates.forEach((value, index) => {
                    const barHeight = (value / maxValue) * (canvas.height - padding * 2) * 0.8;
                    const x = padding + index * barWidth + barWidth * 0.15;
                    const y = canvas.height - padding - barHeight;

                    // ã‚°ãƒ©ãƒ‡ãƒ¼ã‚·ãƒ§ãƒ³è‰²
                    const gradient = ctx.createLinearGradient(0, y, 0, canvas.height - padding);
                    if (index === 0) {
                        gradient.addColorStop(0, '#e74c3c');
                        gradient.addColorStop(1, '#c0392b');
                    } else if (index === 1) {
                        gradient.addColorStop(0, '#3498db');
                        gradient.addColorStop(1, '#2980b9');
                    } else if (index === 2) {
                        gradient.addColorStop(0, '#27ae60');
                        gradient.addColorStop(1, '#229954');
                    } else {
                        gradient.addColorStop(0, '#95a5a6');
                        gradient.addColorStop(1, '#7f8c8d');
                    }

                    ctx.fillStyle = gradient;
                    ctx.fillRect(x, y, barWidth * 0.7, barHeight);

                    // æ•°å€¤è¡¨ç¤º
                    ctx.fillStyle = '#2c3e50';
                    ctx.font = 'bold 12px sans-serif';
                    ctx.textAlign = 'center';
                    ctx.fillText(value.toFixed(1) + '%', x + barWidth * 0.35, y - 10);

                    // ãƒ©ãƒ™ãƒ«è¡¨ç¤º
                    ctx.fillStyle = '#34495e';
                    ctx.font = '10px sans-serif';
                    ctx.fillText(labels[index], x + barWidth * 0.35, canvas.height - 20);
                });

                // ã‚¿ã‚¤ãƒˆãƒ«
                ctx.fillStyle = '#2c3e50';
                ctx.font = 'bold 16px sans-serif';
                ctx.textAlign = 'left';
                ctx.fillText('äººæ°—åˆ¥å‹ç‡(%)', 20, 30);
            }

            // ãƒãƒ£ãƒ¼ãƒˆã‚’æç”»
            drawHorseChart();
