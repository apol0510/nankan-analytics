                ctx.fillText('â€»13-14ç•ªæ ã¯ã‚µãƒ³ãƒ—ãƒ«å°‘', width - 160, 20);
            };

            // äººæ°—åˆ¥æˆç¸¾ã‚°ãƒ©ãƒ•
            const drawPopularityChart = () => {
                const canvas = document.getElementById('popularityChart');
                if (!canvas) {
                    console.log('äººæ°—ãƒãƒ£ãƒ¼ãƒˆã®canvasè¦ç´ ãŒè¦‹ã¤ã‹ã‚Šã¾ã›ã‚“');
